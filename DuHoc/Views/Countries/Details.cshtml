@model DuHoc.Models.Country

@{
    ViewData["Title"] = "Details";
}
@{
    Layout = "_Layout_Admin";
}

<h1>Details</h1>

<div>
    <h4>Country</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Continent)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Continent)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Images)
        </dt>
        <dd class = "col-sm-10">
            <img src="~/images/model.Images" width="300" height="188" class="attachment-full size-full" decoding="async" />
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Introduce)
        </dt>
        <dd class="col-sm-10">
            @Html.Raw(Model.Introduce)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
